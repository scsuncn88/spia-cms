{
  "name": "pudong-airport-cms",
  "version": "2.0.0",
  "private": true,
  "description": "浦东机场小程序后台管理系统演示平台",
  "keywords": ["react", "nextjs", "typescript", "tailwind", "cms", "airport"],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/components tests/lib tests/utils",
    "test:integration": "vitest run tests/integration",
    "test:pages": "vitest run tests/pages",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "test:setup": "playwright install"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3",
    "tailwindcss": "^3.4.6",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.4.39",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.4.0",
    "lucide-react": "^0.408.0",
    "framer-motion": "^11.3.8",
    "recharts": "^2.12.7",
    "react-hook-form": "^7.52.1",
    "zod": "^3.23.8",
    "@hookform/resolvers": "^3.7.0",
    "date-fns": "^3.6.0",
    "zustand": "^4.5.4",
    "@tanstack/react-query": "^5.51.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "react-dropzone": "^14.2.3",
    "class-variance-authority": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "@storybook/addon-essentials": "^8.2.5",
    "@storybook/addon-interactions": "^8.2.5",
    "@storybook/addon-links": "^8.2.5",
    "@storybook/blocks": "^8.2.5",
    "@storybook/nextjs": "^8.2.5",
    "@storybook/react": "^8.2.5",
    "@storybook/test": "^8.2.5",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.45.2",
    "@vitejs/plugin-react": "^4.3.1",
    "vitest": "^2.0.4",
    "jsdom": "^24.1.1"
  }
}